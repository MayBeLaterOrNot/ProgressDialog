<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <packageSources>
        <clear />
        <add key="github" value="https://nuget.pkg.github.com/ax-meyer/index.json" />
        <add key="nuget" value="https://api.nuget.org/index.json" />
    </packageSources>
    <packageSourceCredentials>
        <github>
            <add key="Username" value="${{ github.actor }}" />
            <add key="ClearTextPassword" value="${{ secrets.GITHUB_TOKEN }}" />
        </github>
    </packageSourceCredentials>
</configuration>
